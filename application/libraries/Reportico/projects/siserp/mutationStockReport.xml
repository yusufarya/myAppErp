<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>mutationStockReport</ReportTitle><ReportDescription></ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>.DEFAULT</gridDisplay><gridSortable>.DEFAULT</gridSortable><gridSearchable>.DEFAULT</gridSearchable><gridPageable>.DEFAULT</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT TBL.NAMA, TBL.AWAL, TBL.BELI, TBL.R_JUAL, TBL.TX_PLUS, TBL.P3, TBL.XP_PLUS, TBL.JUAL, TBL.R_BELI, TBL.TX_MIN, TBL.P1, TBL.XP_MIN,  (TBL.AWAL+TBL.BELI+TBL.R_JUAL+TBL.TX_PLUS+TBL.P3+TBL.XP_PLUS-TBL.JUAL-TBL.R_BELI-TBL.TX_MIN-TBL.P1-TBL.XP_MIN) AS AKHIR FROM (
SELECT P1.NAMA, (SELECT IFNULL(SUM(P31.QTY),0) FROM SIS_ACC1812.PROD3 P31 WHERE P31.GROUP = P3.GROUP AND P31.BARANG = P3.BARANG AND FLAG = 1 AND DATE(P31.TGL) BETWEEN '2018-12-01' AND '2018-12-21') AS AWAL, (SELECT IFNULL(SUM(P31.QTY),0) FROM SIS_ACC1812.PROD3 P31 WHERE P31.GROUP = P3.GROUP AND P31.BARANG = P3.BARANG AND FLAG = 3 AND DATE(P31.TGL) BETWEEN '2018-12-01' AND '2018-12-21') AS BELI,
(SELECT IFNULL(SUM(P31.QTY),0) FROM SIS_ACC1812.PROD3 P31 WHERE P31.GROUP = P3.GROUP AND P31.BARANG = P3.BARANG AND FLAG = 4 AND DATE(P31.TGL) BETWEEN '2018-12-01' AND '2018-12-21') AS R_JUAL,
(SELECT IFNULL(SUM(P31.QTY),0) FROM SIS_ACC1812.PROD3 P31 WHERE P31.GROUP = P3.GROUP AND P31.BARANG = P3.BARANG AND FLAG = 2 AND DATE(P31.TGL) BETWEEN '2018-12-01' AND '2018-12-21') AS TX_PLUS,
(SELECT IFNULL(SUM(P31.QTY),0) FROM SIS_ACC1812.PROD3 P31 WHERE P31.GROUP = P3.GROUP AND P31.BARANG = P3.BARANG AND FLAG = 5 AND LEFT(P31.NOMOR,2) = 'P3' AND DATE(P31.TGL) BETWEEN '2018-12-01' AND '2018-12-21') AS P3,
(SELECT IFNULL(SUM(P31.QTY),0) FROM SIS_ACC1812.PROD3 P31 WHERE P31.GROUP = P3.GROUP AND P31.BARANG = P3.BARANG AND FLAG = 5 AND LEFT(P31.NOMOR,2) &lt;&gt; 'P3' AND DATE(P31.TGL) BETWEEN '2018-12-01' AND '2018-12-21') AS XP_PLUS,
(SELECT IFNULL(SUM(P31.QTY),0) FROM SIS_ACC1812.PROD3 P31 WHERE P31.GROUP = P3.GROUP AND P31.BARANG = P3.BARANG AND FLAG = 7 AND DATE(P31.TGL) BETWEEN '2018-12-01' AND '2018-12-21') AS JUAL,
(SELECT IFNULL(SUM(P31.QTY),0) FROM SIS_ACC1812.PROD3 P31 WHERE P31.GROUP = P3.GROUP AND P31.BARANG = P3.BARANG AND FLAG = 8 AND DATE(P31.TGL) BETWEEN '2018-12-01' AND '2018-12-21') AS R_BELI,
(SELECT IFNULL(SUM(P31.QTY),0) FROM SIS_ACC1812.PROD3 P31 WHERE P31.GROUP = P3.GROUP AND P31.BARANG = P3.BARANG AND FLAG = 6 AND DATE(P31.TGL) BETWEEN '2018-12-01' AND '2018-12-21') AS TX_MIN,
(SELECT IFNULL(SUM(P31.QTY),0) FROM SIS_ACC1812.PROD3 P31 WHERE P31.GROUP = P3.GROUP AND P31.BARANG = P3.BARANG AND FLAG = 9 AND LEFT(P31.NOMOR,2) = 'P1' AND DATE(P31.TGL) BETWEEN '2018-12-01' AND '2018-12-21') AS P1, (SELECT IFNULL(SUM(P31.QTY),0) FROM SIS_ACC1812.PROD3 P31 WHERE P31.GROUP = P3.GROUP AND P31.BARANG = P3.BARANG AND FLAG = 9 AND LEFT(P31.NOMOR,2) &lt;&gt; 'P1' AND DATE(P31.TGL) BETWEEN '2018-12-01' AND '2018-12-21') AS XP_MIN FROM SIS_ACC1812.PROD3 P3 LEFT JOIN PROD1 P1 ON P1.GROUP = P3.GROUP AND P1.KODE AND P3.BARANG GROUP BY P1.NAMA) TBL ORDER BY TBL.NAMA</SQLRaw></SQL><QueryColumns><QueryColumn><Name>NAMA</Name><TableName>TBL</TableName><ColumnName>NAMA</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>AWAL</Name><TableName>TBL</TableName><ColumnName>AWAL</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>right</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>BELI</Name><TableName>TBL</TableName><ColumnName>BELI</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column